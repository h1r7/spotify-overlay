{
  "name": "spotify-overlay",
  "version": "0.1.0",
  "private": true,
  "bin": "launcher/index.js",
  "scripts": {
    "build": "next build",
    "dev": "next dev -p 6974",
    "lint": "eslint .",
    "start": "next start -p 6974",
    "make-exe": "xcopy public .next\\standalone\\public /E /I /Y && xcopy .next\\static .next\\standalone\\.next\\static /E /I /Y && pkg . --compress GZip --output dist/FLUX.exe --icon launcher/icon.ico",
    "deploy": "npm run build && npm run make-exe && copy FLUX-setup.bat dist\\FLUX-setup.bat"
  },
  "pkg": {
    "scripts": "launcher/index.js",
    "assets": [
      ".next/standalone/**/*",
      "public/**/*",
      "launcher/pkg-patches.js"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
    "autoprefixer": "^10.4.20",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "next": "16.0.10",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.9",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "pkg": "^5.8.1",
    "postcss": "^8.5",
    "tailwindcss": "^4.1.9",
    "typescript": "^5"
  }
}